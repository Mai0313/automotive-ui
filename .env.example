# Using OpenAI Endpoint
EXPO_PUBLIC_OPENAI_API_TYPE=...
EXPO_PUBLIC_OPENAI_BASE_URL=...
EXPO_PUBLIC_OPENAI_API_KEY=...
EXPO_PUBLIC_OPENAI_API_VERSION=...
EXPO_PUBLIC_OPENAI_MODEL=...

# Using Azure OpenAI Endpoint
# EXPO_PUBLIC_OPENAI_API_TYPE=azure
# EXPO_PUBLIC_OPENAI_BASE_URL=https://...openai.azure.com
# EXPO_PUBLIC_OPENAI_API_KEY=...
# EXPO_PUBLIC_OPENAI_API_VERSION=2025-04-01-preview
# EXPO_PUBLIC_OPENAI_MODEL=aide-gpt-4.1
# EXPO_PUBLIC_OPENAI_DEPLOYMENT_NAME=aide-gpt-4.1

# Backend Server Configuration (REQUIRED)
# WebSocket server URL for real-time communication
EXPO_PUBLIC_WS_SERVER_URL=ws://localhost:4000
# HTTP server URL for REST API fallback
EXPO_PUBLIC_HTTP_SERVER_URL=http://localhost:4001
# Realtime voice WebSocket server URL
EXPO_PUBLIC_REALTIME_VOICE_URL=ws://localhost:8100/ws

# Server binding URLs (used by server.js)
# localhost will be automatically converted to 0.0.0.0 for server binding
# This allows servers to accept connections from any network interface
WS_SERVER_URL=ws://localhost:4000
HTTP_SERVER_URL=http://localhost:4001
